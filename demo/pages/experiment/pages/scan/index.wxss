.log-list {
  display: flex;
  flex-direction: column;
  padding: 40rpx;
}
.log-item {
  margin: 10rpx;
}

.item{
  margin: 2px 0px;
  padding: 10px 80%;
  border: 1px solid black;
  background-color: rgb(160, 230, 236);
}

.hover{
  background-color: rgb(198, 239, 243);
}

.img{
  border: black 1px solid;
  width: 48px;
  height: 48px;
}

.webview{
  border: rgb(80, 76, 76) 1px solid;
  width: 100px;
  height: 100px;
}
